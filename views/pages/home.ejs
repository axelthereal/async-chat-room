<%- include("../headers/header"); %>

<script>
    const { createApp } = Vue;
</script>

<main id="homeApp" class="container">
<h1>{{ test }}</h1>
</main>

<%- include("../headers/pageLoader"); %>
<script>
    const homeApp = createApp({
        data: function(){
          return { 
            test: "Home Page"
          }
        },
        methods: { 
          displayBody(){
             document.querySelector("#homeApp").style.display = "block"; 
             document.querySelector("#loaderApp").style.display = "none"; 
          }
        },
       created() {
         document.querySelector("#homeApp").style.display = "none";
         setTimeout(this.displayBody, 4800);
       },
     }).mount("#homeApp");
</script>
